@model APIDashboard.Models.DashBoardProdREST
@{
    ViewData["Title"] = "Production";
}

<environment include="Development">
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
	<link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/apidashboard.css" />
</environment>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js"></script>
<script src='https://kit.fontawesome.com/a076d05399.js'></script>
<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>

<partial name="_Header/_HeaderAPIREST" />

<div class="container-fluid">
	<partial name="_ALLAPI/_ImportantInformationsCanvas" />
	<partial name="_ALLAPI/_APIIssues" />
	<div class="row">
        <div class="col-xl-12 col-lg-12">
            <div class="card shadow mb-4">
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                    @foreach (Methods myName in Model._Methods)
                    {
                        <p data-toggle="collapse" class="card-header-link-title_SBMAPIREST">@myName.TitleMethod</p>
                    }
                    <partial name="_Popup/_PopupSBM" />
                </div>
                <partial name="_APIREST/_StatusByMethod" />
            </div>
        </div>
    </div>
    <div class="text-center" role="group" id="blocklinkapi">
        <a class="btn btn-secondary" asp-area="" asp-controller="APISC" asp-action="Production" role="button" id="blocklinkapi1">Go to API Sales Channel &gt;</a>
        <a class="btn btn-primary" asp-area="" asp-controller="APISOAP" asp-action="Production" role="button" id="blocklinkapi2">Go to API SOAP &gt;</a>
    </div>
</div>
<button onclick="topFunction()" id="myBtn" title="Back to top"><i class="fas fa-arrow-up"></i></button>
<script type="text/javascript" src="~/js/site.js"></script>