                    }
                </div>
                <div id="issues" class="panel-collapse collapse">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th class="issue-font" scope="col">#</th>
                                <th class="issue-font" scope="col">Issue</th>
                                <th class="issue-font-w" scope="col">Status</th>
                                <th class="issue-font" scope="col">Planned</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (Issues myIssue in Model._Issues)
                            {
                                var style = (myIssue.StateIssue == "DONE") ? "color:#28a745;font-weight:bold;text-align:center;font-size:14px!important;font-family: 'Montserrat-Light', sans-serif" : "color:#007bff;font-weight:bold;text-align:center;font-size:14px!important;font-family: 'Montserrat-Light', sans-serif";
                                var style1 = (myIssue.StateIssue == "Nothing to report") ? "color:#28a745;font-weight:bold;text-align:center;font-size:14px!important;font-family: 'Montserrat-Light', sans-serif" : "";
                                <tr>
                                    <th class="issue-font-content" scope="row">@myIssue.NumberIssue</th>
                                    <td class="issue-font-content-j">@myIssue.ExplicationIssue</td>
                                    <td style="@style;@style1">@myIssue.StateIssue</td>
                                    <td class="issue-font-content">@myIssue.DateOfResolutionIssue</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
